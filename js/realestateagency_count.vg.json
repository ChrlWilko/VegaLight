        {
            "aggregate": [
                {
                    "op": "count",
                    "as": "count"
                }
            ],
            "groupby": [
                "SellerG"
            ]
        },
        {
            "window": [
                {
                    "op": "rank",
                    "as": "rank"
                }
            ],
            "sort": [
                {
                    "field": "count",
                    "order": "descending"
                }
            ]
        },
        {
            "filter": "datum.rank <= 10"
        }
    ],
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "SellerG",
            "type": "nominal",
            "sort": {
                "field": "count",
                "op": "sum",
                "order": "descending"
            },
            "axis": {
                "title": "Real Estate Agency",
                "labelLimit": 300
            }
        },
        "y": {
            "field": "count",
            "type": "quantitative",
            "axis": {
                "title": "Number of Properties Sold"
            }
        },
        "color": {
            "value": "purple"
        },
        "tooltip": [
            {
                "field": "SellerG",
                "title": "Real Estate Agency",
                "type": "nominal"
            },
            {
                "field": "count",
